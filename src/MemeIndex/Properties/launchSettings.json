    },
    "files": {
      "commandName": "Project",
      "commandLineArgs": "-P args.txt",
      "workingDirectory": "$(OutDir)"
    },
    "demo": {
      "commandName": "Project",
      "commandLineArgs": "-D args.txt",
      "workingDirectory": "$(OutDir)"
    }
  }
